<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.web.WebView?>

<AnchorPane minHeight="530.0" prefWidth="850.0" style="-fx-background-color: 10; -fx-border-radius: 10;" stylesheets="@my.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
    <children>
        <WebView fx:id="webView" layoutX="1.0" layoutY="44.0" prefHeight="441.0" prefWidth="519.0" stylesheets="@my.css" />
        <ToolBar layoutY="1.0" prefHeight="40.0" prefWidth="520.0">
            <items>
                <Button fx:id="backButton" mnemonicParsing="false" text="&lt;-" />
                <Button fx:id="forwardButton" mnemonicParsing="false" text="-&gt;" />
                <Button fx:id="goButton" defaultButton="true" mnemonicParsing="false" text="GO" />
                <TextField fx:id="addresBar" prefHeight="25.0" prefWidth="390.0" promptText="Input here full address" />
            </items>
        </ToolBar>
        <TableView fx:id="tableView" layoutX="520.0" layoutY="38.0" prefHeight="385.0" prefWidth="335.0" style="-fx-border-color: #9F9F9F;">
            <columns>

                <TableColumn fx:id="tableViewColumName" prefWidth="169.0" text="Name" />
                <TableColumn fx:id="tableViewColumLink" prefWidth="165.0" text="Link" />
            </columns>
        </TableView>
        <Pane layoutX="520.0" layoutY="422.0" prefHeight="47.0" prefWidth="335.0" style="-fx-background-color: #E9E9E9; -fx-border-color: #9F9F9F;">
            <children>
                <TextField fx:id="nameField" layoutX="6.0" layoutY="11.0" prefHeight="25.0" prefWidth="159.0" promptText="Name" style="-fx-border-color: #9F9F9F;">
                    <opaqueInsets>
                        <Insets left="10.0" />
                    </opaqueInsets>
                </TextField>
                <TextField fx:id="linkField" layoutX="170.0" layoutY="11.0" prefHeight="25.0" prefWidth="160.0" promptText="Link" style="-fx-border-color: #9F9F9F;" />
            </children>
            <effect>
                <DropShadow height="3.0" radius="1.0" width="3.0" />
            </effect>
        </Pane>
        <Pane layoutX="520.0" layoutY="468.0" prefHeight="74.0" prefWidth="335.0" style="-fx-background-color: #DDDDDD; -fx-border-color: #9F9F9F;">
            <children>
                <Button fx:id="addButton" defaultButton="true" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" prefHeight="47.0" prefWidth="73.0" style="-fx-border-radius: 5; -fx-background-radius: 5;" text="Add">
                    <effect>
                        <DropShadow color="#afc7d7" height="12.0" radius="5.5" width="12.0" />
                    </effect>
                    <font>
                        <Font size="14.0" />
                    </font>
                </Button>
                <Button fx:id="refreshButton" defaultButton="true" layoutX="132.0" layoutY="14.0" mnemonicParsing="false" prefHeight="47.0" prefWidth="73.0" style="-fx-background-radius: 115; -fx-border-radius: 115;" text="Refresh">
                    <effect>
                        <DropShadow color="#afc7d7" height="12.0" radius="5.5" width="12.0" />
                    </effect>
                    <font>
                        <Font size="14.0" />
                    </font>
                </Button>
                <Button fx:id="deleteButton" defaultButton="true" layoutX="248.0" layoutY="14.0" mnemonicParsing="false" prefHeight="47.0" prefWidth="73.0" style="-fx-background-radius: 5; -fx-border-radius: 5;" text="Delete">
                    <effect>
                        <DropShadow color="#afc7d7" height="12.0" radius="5.5" width="12.0" />
                    </effect>
                    <font>
                        <Font size="14.0" />
                    </font>
                </Button>
            </children>
            <effect>
                <DropShadow height="4.0" radius="1.5" width="4.0" />
            </effect>
        </Pane>
        <ToolBar layoutX="520.0" layoutY="1.0" prefHeight="40.0" prefWidth="335.0">
            <items>
                <TextField fx:id="searchField" onKeyReleased="#searchName" prefHeight="25.0" prefWidth="321.0" promptText="Search entry" />
            </items>
        </ToolBar>
        <Separator layoutX="517.0" orientation="VERTICAL" prefHeight="40.0" prefWidth="9.0" />
        <Button fx:id="exportButton" layoutX="403.0" layoutY="505.0" mnemonicParsing="false" text="Export to Excel" />
        <ComboBox fx:id="searchSystemBox" layoutX="242.0" layoutY="505.0" prefWidth="150.0" promptText=" " />
        <Button fx:id="editButton" layoutX="169.0" layoutY="505.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="65.0" text="Edit" />
        <Button fx:id="saveButton" layoutX="117.0" layoutY="505.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="52.0" text="Save" />
    </children>
    <cursor>
        <Cursor fx:constant="DEFAULT" />
    </cursor>
</AnchorPane>
